  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "npx serve@latest out -p 31000",
    "lint": "next lint",
    "clean_windows": "(if exist node_modules rd /s /q node_modules) && (if exist node_modules rd /s /q .next) && (if exist .next rd /s /q .next) && (if exist out rd /s /q out) && (if exist package-lock.json del package-lock.json)",

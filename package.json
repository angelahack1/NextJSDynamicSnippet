    "next": "15.3.5",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "24.0.11",
    "@types/react": "19.1.8",
    "eslint": "^8",
    "eslint-config-next": "14.2.3"
  }
}
